{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python@4.3.0"
    }
  ],
  "routes": [
    {
      "src": "/track-view",
      "dest": "app.py"
    },
    {
      "src": "/stats",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
