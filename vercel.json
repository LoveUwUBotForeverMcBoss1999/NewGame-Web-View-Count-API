{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@4.3.0",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/track-view",
      "dest": "/api/index.py"
    },
    {
      "src": "/stats",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}