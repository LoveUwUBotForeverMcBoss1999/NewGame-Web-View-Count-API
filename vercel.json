{
  "version": 2,
  "functions": {
    "app.py": {
      "runtime": "@vercel/python@4.3.0",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/track-view",
      "dest": "app.py"
    },
    {
      "src": "/stats",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
